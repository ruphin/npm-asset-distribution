{
  "name": "npm-asset-distrubution",
  "description": "Testing asset distribution over NPM",
  "main": "npm-asset-distribution.js",
  "repository": "git@github.com:ruphin/npm-asset-distribution.git",
  "author": "Goffert van Gool",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "browser-sync": "^2.18.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0"
  },
  "scripts": {
    "build": "gulp",
    "dev": "gulp serve",
    "publish": "git add npm-asset-distribution.js"
  },
  "install":
    "wget --no-check-certificate https://github.com/ruphin/npm-asset-distrubution/tarball/$NPM_PACKAGE_VERSION -O - | tar xz --strip-components 1  --wildcards --no-anchored 'assets/*'"
}
